{
    "name": "@jx/iter",
    "version": "0.0.1",
    "description": "train.js is a library of iterator helper methods (for both sync and async iterators)",
    "main": "index.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "node --harmony-async-iteration test.js"
    },
    "author": "James \"The Jamesernator\" Browning",
    "license": "MIT",
    "devDependencies": {
        "ava": "^0.24.0",
        "dedent": "^0.7.0",
        "es6-array-map": "latest",
        "es6-array-set": "latest",
        "folder-module": "latest"
    },
    "ava": {
        "files": [
            "tests/sync/*.js",
            "tests/async/*.js"
        ],
        "source": [
            "src/**/*.mjs"
        ],
        "powerAssert": true,
        "require": "@std/esm",
        "modules": false,
        "babel": {
            "presets": [
                "@ava/stage-4"
            ],
            "parserOpts": {
                "plugins": [
                    "asyncGenerators"
                ]
            },
            "sourceType": "module"
        }
    },
    "dependencies": {
        "esm": "latest",
        "babel-eslint": "^8.2.2",
        "eslint": "^4.18.2"
    }
}
