{
    "name": "@jsxt/iter",
    "type": "module",
    "exports": {
        "./async": "./dist/async.js",
        "./sync": "./dist/sync.js",
        "./async/*": "./dist/async/*.js",
        "./sync/*": "./dist/sync/*.js"
    },
    "version": "1.1.3",
    "description": "This is a library of helper methods for dealing with Iterables",
    "scripts": {
        "build": "rm -rf dist/ && tsc && node build.cjs",
        "lint": "eslint **/*.ts",
        "lint:fix": "eslint --fix **/*.ts",
        "test": "npm run build && cd dist/ && node --experimental-modules runTests.js"
    },
    "author": "James \"The Jamesernator\" Browning",
    "license": "MIT",
    "repository": "https://github.com/jsxt/iter",
    "devDependencies": {
        "@jsxt/eslint-config-typescript": "^4.8.0",
        "@types/glob": "^7.1.1",
        "@types/node": "^14.14.9",
        "ava": "^3.13.0",
        "chalk": "^3.0.0",
        "folder-module": "^2.0.4",
        "glob": "^7.1.6",
        "typescript": "^4.1.2"
    },
    "files": [
        "build.cjs",
        "tsconfig.json",
        "LICENSE",
        "dist/**/*",
        "docs/**/*",
        "src/**/*"
    ]
}
