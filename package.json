{
  "name": "train.js",
  "version": "0.0.1",
  "description": "train.js is a library of iterator helper methods (for both sync and async iterators)",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "ava --verbose"
  },
  "author": "James \"The Jamesernator\" Browning",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-core": "latest",
    "babel-preset-stage-3": "latest",
    "es6-array-map": "latest",
    "es6-array-set": "latest"
  },
  "ava": {
